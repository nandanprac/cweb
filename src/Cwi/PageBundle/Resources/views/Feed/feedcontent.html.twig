{% for feed in feeds %}
    <div class="row mtminus10">
        <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">
            <img class="img-responsive img-circle" src="{{ asset('bundles/cwipage/images/doctor.jpeg') }}" width="40" alt="">
        </div>
        <div class="col-xs-21 col-sm-21 col-md-21 col-lg-21 paddingleftzero shdivsp">
            <p class="greysdark fw6 fs16 mtminus5">{{ feed.subject }}</p>
            <p class="fw4 fs14">
                {{ feed.text }}
            </p>
            <p class="greylight fs12 mbminus5">{{ feed.view_count }} views . {{ feed.bookmark_count }} bookmarks</p>
            <hr>
        </div>
    </div>
{% else %}
    <div class="row mtminus10">
        <p class="fw4 fs14">No Feeds Found</p>
    </div>
{% endfor %}